from random import randint
from time import sleep
computador = randint(0, 5)
print('-*-' * 15 )
print('Vou pensar em um numero...')
print('-*-' * 15)
Jogador = int(input('Tente adivinhar em que numero estou pensando?'))
print('PROCESSANDO..')
sleep(5)
print('-*-' * 15)
if Jogador == computador:
  print('VocÃª adivinhou, parabens')
else:
  print('Tente de novo')
print('-*-' * 15)
