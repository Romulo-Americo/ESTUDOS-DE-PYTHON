from datetime import date
atual = date.today().year
soma_de_maior_idade = 0
soma_menor_de_idade = 0
for pessoas in range(1, 8):
    idade = int(input('Digite seu ano de nascimento: '))
    if atual - idade >= 18:
        soma_de_maior_idade += 1
    else:
        soma_menor_de_idade += 1
print(soma_de_maior_idade)
print(soma_menor_de_idade)
